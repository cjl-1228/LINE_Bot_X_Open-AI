        "import openai\n",
        "openai.api_key = \""\n",
        "def askchatgpt(Question):\n",
        "    response = openai.Completion.create(\n",
        "    model=\"text-davinci-003\",\n",
        "    prompt=Question,\n",
        "    temperature=0,\n",
        "    max_tokens=500,\n",
        "    top_p=1,\n",
        "    frequency_penalty=0.5,\n",
        "    presence_penalty=0\n",
        "    )\n",
        "    return response[\"choices\"][0][\"text\"].strip()"
      ],
